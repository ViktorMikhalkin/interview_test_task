{
  "name": "namecheap-qa-auto-task",
  "version": "1.0.0",
  "description": "QA Automation Test",
  "scripts": {
    "build": "npm-run-all clean-dist tsc lint",
    "clean": "npm-run-all clean-reports clean-logs clean-runtimedata",
    "clean-dist": "rimraf ./dist",
    "clean-reports": "rimraf ./run-results/reports",
    "clean-logs": "rimraf ./run-results/logs",
    "clean-runtimedata": "rimraf ./runtime-data",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "mkdir-run-results": "ts-node src/taf-support/scripts/mk-run-results-dir.ts",
    "mkdir-runtime-data": "ts-node src/taf-support/scripts/mk-runtime-dir.ts",
    "postinstall": "npm run wd-mgr-upd",
    "protractor": "protractor ./dist/protractor.conf.js",
    "tsc": "tsc",
    "test": "npm run protractor",
    "wd-mgr-upd": "webdriver-manager update"
  },
  "keywords": [],
  "author": "Viktor Mikhalkin",
  "license": "ISC",
  "private": true,
  "engines": {
    "node": ">=12.4.0",
    "npm": ">=6.9.0"
  },
  "dependencies": {
    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "^7.1.0",
    "@types/cucumber": "^4.0.5",
    "@types/deep-equal-in-any-order": "^1.0.0",
    "@types/download": "^6.2.4",
    "@types/node": "^12.0.8",
    "@types/shelljs": "^0.8.5",
    "@types/unzip": "^0.1.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-exclude": "^2.0.1",
    "chai-sorted": "^0.2.0",
    "commander": "^2.20.0",
    "config": "^3.1.0",
    "cucumber": "^5.1.0",
    "deep-equal-in-any-order": "^1.0.13",
    "download": "^7.1.0",
    "moment": "^2.24.0",
    "npm-run-all": "^4.1.5",
    "protractor": "^5.4.2",
    "protractor-cucumber-framework": "^6.1.2",
    "protractor-multiple-cucumber-html-reporter-plugin": "^1.8.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^2.6.3",
    "shelljs": "^0.8.3",
    "ts-node": "^8.2.0",
    "typescript": "^3.5.1",
    "unzip": "^0.1.11"
  },
  "devDependencies": {
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-plugin-prettier": "^2.0.1"
  }
}
